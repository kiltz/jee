				<xsd:sequence>
<xsd:element name="Fragenkomplex" type="allgemein:CT_Fragenkomplex" minOccurs="0" maxOccurs="unbounded"/>
</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_KapitalbildendeVersicherung">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Umfasst reine Kapitalbildende Versicherungen</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_Elementarprodukt">
				<xsd:sequence>
<xsd:element name="FlexibleAuszahlphase" type="taa:CT_Zeitraum" minOccurs="0" maxOccurs="1"/>
