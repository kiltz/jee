			<xsd:extension base="partner:CT_Partnerrolle">
				<xsd:sequence/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_UnfallAbsicherung">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Umfasst Produkte der Unfallabsicherung.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_Elementarprodukt">
				<xsd:sequence/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_TodesfallAbsicherung">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Umfasst Produkte der Todesfallabsicherung (Risikolebensversicherung).</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_Elementarprodukt">
				<xsd:sequence>
<xsd:element name="Waisenrentenschlussalter" type="daten:ST_Ganzzahl" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation xml:lang="de">Der Wert gibt an, bis zum Erreichen welchen Alters eine Waisenrente ausbezahlt wird.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_PflegeAbsicherung">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Umfasst Produkte zur Pflegeabsicherung.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CT_Elementarprodukt">
				<xsd:sequence>
<xsd:element name="Karrenzzeit" type="taa:CT_Dauer" minOccurs="0" maxOccurs="1"/>
<xsd:element name="Aufschubzeit" type="taa:CT_Dauer" minOccurs="0" maxOccurs="1"/>
</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_Produkt">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Daten zum Produkt</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="taa:CT_Produkt">
				<xsd:sequence>
<xsd:element name="Tarifbezeichnung" type="daten:STE_Tarifbezeichnung" minOccurs="0" maxOccurs="1">
