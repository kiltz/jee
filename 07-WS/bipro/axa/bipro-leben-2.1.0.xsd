						</xsd:annotation>
					</xsd:element>
<xsd:element name="BAV" type="CT_BAV" minOccurs="0" maxOccurs="1"/>
<xsd:element name="Verlauf" type="CT_Verlauf" minOccurs="0" maxOccurs="unbounded"/>
