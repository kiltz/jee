	<xsd:complexType name="CT_KonditionErweiterung" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Individuelle Rabattierung der AXA</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="allgemein:CT_Erweiterung">
				<xsd:sequence>
					<xsd:element name="RabattTyp" type="ST_RabattTyp"/>
					<xsd:element name="RabattVerein" type="ST_RabattVerein"/>
					<xsd:element name="RabattStufe" type="ST_RabattStufe"/>
					<xsd:element name="Stueckkosten" type="daten:ST_JaNein" minOccurs="0"/>
					<xsd:element name="ZugangZumKollektivvertragBeantragen" type="daten:ST_JaNein" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_FondsportfolioErweiterung" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Infos zur Wertentwicklung</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="allgemein:CT_Erweiterung">
				<xsd:sequence maxOccurs="3">
					<xsd:element name="Wertentwicklung" type="CT_Wertentwicklung"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="ST_Finanzierung_bAV">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Finanzierungsart der bAV</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation xml:lang="de">ArbeitnehmerFinanzierung</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation xml:lang="de">MischfinanzierungOhneUmwandlungVL</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4">
				<xsd:annotation>
					<xsd:documentation xml:lang="de">bAVstattGehaltserhoehung</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="de">ArbeitgeberFinanzierung</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_VorgabeErweiterung">
		<xsd:annotation>
			<xsd:documentation>Informationen für eine Rechenvorgabe</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="allgemein:CT_Erweiterung">
				<xsd:sequence>
					<xsd:element name="Zielalter" type="daten:ST_Ganzzahl" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Alternativer Zeitpunkt für die  Angabe der Versicherungsleistungen</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ZinsBeitragsverrechnungskonto" type="daten:ST_Dezimalzahl" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Zinssatz des Beitragsverrechnungskontos</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Finanzierung_bAV" type="ST_Finanzierung_bAV" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Finanzierungsart der bAV</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Optimierung" type="daten:ST_JaNein" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Optimierung durch den Rechenkern zulässig?</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="ST_KontoVerwendung">
		<xsd:annotation>
			<xsd:documentation>Verwendungsart eines Kontos</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Beitragskonto"/>
			<xsd:enumeration value="Rentenkonto"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CT_BankverbindungErweiterung">
		<xsd:annotation>
			<xsd:documentation>Zusaetzliche Informationenen zur Bankverbindung</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="allgemein:CT_Erweiterung">
				<xsd:sequence>
					<xsd:element name="AbweichenderKontoinhaber" type="daten:ST_JaNein" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Abweichernder Kontoinhaber</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="KontoVerwendung" type="ST_KontoVerwendung" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Verwendungsart eines Kontos</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SitzDerBank" type="daten:ST_Text" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Sitz der Bank</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CT_Wertentwicklung" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Angabe der Wertentwicklung</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="allgemein:CT_Objekt">
				<xsd:sequence>
					<xsd:element name="Wertentwicklung" type="daten:STE_LeistungSchluessel"/>
					<xsd:element name="Zinssatz" type="daten:ST_Prozent"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
