				<xsd:enumeration value="Beitragsverrechnung"/>
				<xsd:enumeration value="Turbodynamik"/>
				<xsd:enumeration value="DynamischeGewinnrente"/>
				<xsd:enumeration value="ErhoehteStartrente"/>
				<xsd:enumeration value="VerzinslicheAnsammlung"/>
				<xsd:enumeration value="VariableGewinnrente"/>
				<xsd:enumeration value="Investmentbonus"/>
				<xsd:enumeration value="Todesfallbonus"/>
