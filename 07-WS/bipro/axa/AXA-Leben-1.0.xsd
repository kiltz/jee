	</xsd:complexType>
	<xsd:complexType name="STE_VorgabeBerechnungErweiterung">
		<xsd:annotation>
			<xsd:documentation>Berechnungsvorgaben</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="daten:STE_VorgabeBerechnung">
				<xsd:enumeration value="Gesamtbeitrag"/>
				<xsd:enumeration value="HVBeitrag"/>
				<xsd:enumeration value="Garantierente"/>
				<xsd:enumeration value="Investmentrente"/>
				<xsd:enumeration value="Kapitalabfindung"/>
				<xsd:enumeration value="Optimalbeitrag"/>
				<xsd:enumeration value="Mindesteigenbeitrag"/>
				<xsd:enumeration value="Maximalaufwand"/>
				<xsd:enumeration value="KonstanterAufwand"/>
				<xsd:enumeration value="Zulagenverrechnung"/>
				<xsd:enumeration value="BURenteBeiMinimalerSparleistung"/>
				<xsd:enumeration value="RenteMitUeberschuss"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="STE_VersicherungssummeOderLeistungErweiterung">
		<xsd:annotation>
			<xsd:documentation>Berechnungsvorgaben</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="daten:STE_VersicherungssummeOderLeistung">
				<xsd:enumeration value="GarantierteBURente"/>
				<xsd:enumeration value="BURente_inkl_Bonus"/>
				<xsd:enumeration value="MaximaleBURente"/>
				<xsd:enumeration value="Garantieleistung"/>
				<xsd:enumeration value="ZuzahlungAblaufleistung"/>
				<xsd:enumeration value="ZuzahlungRente"/>
				<xsd:enumeration value="GarantierteRenteAusEigenbeitraegen"/>
				<xsd:enumeration value="GarantierteRenteAusZulagen"/>
				<xsd:enumeration value="BURenteNachAnfangsphase"/>
				<xsd:enumeration value="BURente_inkl_BonusNachAnfangsphase"/>
				<xsd:enumeration value="Hinterbliebenenrente"/>
				<xsd:enumeration value="Halbwaisenrente"/>
				<xsd:enumeration value="Vollwaisenrente"/>
				<xsd:enumeration value="Waisenrente"/>
			</xsd:restriction>
			<!-- zulässige Vorgaben für BU-Rente sind: -->
			<!--    Garantierte monatliche BU-Rente -->
			<!--    Monatliche BU-Rente inkl. Bonus -->
			<!--    Garantierte jährliche BU-Rente in % der Beitragssumme -->
			<!--    Jährliche BU-Rente inkl. Bonus in % der Beitragssumme -->
			<!--    Maximal mögliche BU-Rente -->
			<!-- Zahlweise und Werteinheit werden in separaten Attributen angegeben -->
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="STE_KonditionErweiterung">
		<xsd:annotation>
			<xsd:documentation>Art der Zu- oder Abschlaege</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="daten:STE_Kondition">
				<xsd:enumeration value="BUZ_Risikozuschlag"/>
				<xsd:enumeration value="RZ_RisikozuschlagKonventionell"/>
				<xsd:enumeration value="RZ_RisikozuschlagFondsgebunden"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="STE_DynamikErweiterung">
		<xsd:annotation>
			<xsd:documentation>Dynamikform</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="daten:STE_Dynamik">
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>AV-Anpassung mind. 5%</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="2">
					<xsd:annotation>
						<xsd:documentation>Beitragssatz angeben</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="3">
					<xsd:annotation>
						<xsd:documentation>Jaehrliche Erhoehung</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="4">
					<xsd:annotation>
						<xsd:documentation>Gemaess gesetzl. RV-Beitrag</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="5">
					<xsd:annotation>
						<xsd:documentation>AV-Anpassung</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
